{
  "name": "nexusgraph",
  "workspaces": [
    "./packages/nexusgraph-app",
    "./packages/nexusgraph-db",
    "./packages/nexusgraph-graph",
    "./packages/nexusgraph-nlp",
    "./packages/nexusgraph-oauth",
    "./packages/nexusgraph-redux"
  ],
  "version": "0.1.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "description": "Find solution via Knowledge Graph and Refine the solution with Data",
  "scripts": {
    "start": "node scripts/start.js",
    "start:ai": "json-server --watch packages/nexusgraph-nlp/src/json-server/ner.json --port 3001",
    "start:graph-api": "node packages/nexusgraph-db/src/graph/json-graphql-server/server/server.js",
    "build": "node scripts/build.js",
    "test": "jest",
    "cypress:open": "cypress open",
    "e2e": "cypress run --browser chrome",
    "wait-on-ai": "wait-on -c ./scripts/wait-on-config.js http-get://localhost:3001/",
    "wait-on-dev": "wait-on -c ./scripts/wait-on-config.js http-get://localhost:3000/",
    "wait-on-prod": "wait-on -c ./scripts/wait-on-config.js http-get://localhost:3000/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QubitPi/nexusgraph.git"
